Some output based questions-
 // Code 1:
function foo(){
 setTimeout(()=>{
  console.log(a);
  console.log(b);
 },100);
 var a = 2;
 let b = 12;
}

foo();

// Code 2:
function foo(){
 for(var i = 0; i < 3; i++){
  setTimeout(()=> console.log(i),2000);
 }
}
foo();


Promise.resolve(10)
    .then((r) => {
        throw (r + 10)
    })
    .catch((e) => {
        return e + 24;
    })
    .then(console.log)
    .catch((e) => {
        console.log(20 + e)
    })

